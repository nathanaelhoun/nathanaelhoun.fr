{
  "name": "nathanaelhoun.fr",
  "scripts": {
    "mustache:fr":"mustache src/fr.json src/template.mustache > index.big.html" ,
    "mustache:en": "mustache src/en.json src/template.mustache > en.big.html",
    "minify:fr": "minify index.big.html > docs/index.html",
    "minify:en": "minify en.big.html > docs/en.html",
    "build": "(npm run mustache:fr & npm run mustache:en) && (npm run minify:fr & npm run minify:en)",
    "clean": "rm *.big.html"
  },
  "dependencies": {
    "minify": "^5.1.1",
    "mustache": "^4.0.1"
  }
}
